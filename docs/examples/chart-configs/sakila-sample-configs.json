{
  "description": "Sample JSON chart configurations for testing the Optiqo Chart Builder with the Sakila database",
  "version": "2.1.0",
  "note": "Copy the 'config' object (not the wrapper) to paste into the debug panel",
  "configs": [
    {
      "name": "1. Column Chart: Films by Category",
      "description": "Shows film count per category - Sports has 74, Music has 51",
      "config": {
        "chartType": "column",
        "chartTitle": "Films by Category",
        "xDimensions": [
          {
            "fieldId": "name",
            "name": "name",
            "label": "Category",
            "type": "varchar",
            "table": "category"
          }
        ],
        "yMetrics": [
          {
            "fieldId": "film_id",
            "name": "film_id",
            "label": "Film Count",
            "type": "smallint",
            "table": "film_category",
            "aggregation": "COUNT",
            "isNumeric": true
          }
        ],
        "breakdowns": [],
        "filters": [],
        "joins": [
          {
            "constraintName": "fk_film_category_category",
            "sourceTable": "film_category",
            "targetTable": "category",
            "joinType": "inner",
            "columnPairs": [{"position": 1, "sourceColumn": "category_id", "targetColumn": "category_id"}]
          }
        ],
        "appearance": {
          "showLegend": false,
          "showLabels": true
        }
      }
    },
    {
      "name": "2. Pie Chart: Films by Rating",
      "description": "Shows film count per rating - PG-13 leads with 223, G has 178",
      "config": {
        "chartType": "pie",
        "chartTitle": "Films by Rating",
        "xDimensions": [
          {
            "fieldId": "rating",
            "name": "rating",
            "label": "Rating",
            "type": "enum",
            "table": "film"
          }
        ],
        "yMetrics": [
          {
            "fieldId": "film_id",
            "name": "film_id",
            "label": "Film Count",
            "type": "smallint",
            "table": "film",
            "aggregation": "COUNT",
            "isNumeric": true
          }
        ],
        "breakdowns": [],
        "filters": [],
        "joins": [],
        "appearance": {
          "showLegend": true,
          "legendPosition": "right",
          "showLabels": true
        }
      }
    },
    {
      "name": "3. Column Chart: Films by Rental Duration",
      "description": "Shows film count per rental duration (3-7 days). Duration 6 has 212 films.",
      "config": {
        "chartType": "column",
